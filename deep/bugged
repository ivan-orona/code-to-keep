   //This will prompt the user to input their name.
   public static void bugged01()
   {
       String name1; //holds the first name
       String name2; //holds the second name
       //prompts the user.
       Scanner user_input = new Scanner (System.in);
       System.out.println("What is your name?");
       name1 = user_input.next();
       System.out.println("Hello, I am " + name1 + " and"
               + "this is my program." );
       System.out.println("Who is the second user?");
       name2 = user_input.next();
       //prints out the completed form.
       System.out.println("Hell, I am " + name2 + " and I "
               + "am checking this program "
               + "for " + name1 + ".");
   }
